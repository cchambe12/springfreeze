library(BayesFactor)
jzs_corbf=function(r,n){
int=function(r,n,g){
(1+g)^((n-2)/2)*(1+(1-r^2)*g)^(-(n-1)/2)*
g^(-3/2)*exp(-n/(2*g))};
bfl0=sqrt((n/2))/gamma(1/2)*integrate(int,lower=0,upper=Inf,r=r,n=n)$value;
return(bfl0)}
r <- 0.9497443
n <- 5
jzs_corbf(r,n)
